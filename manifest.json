{
  "name": "Cisco AEM Tool",
  "manifest_version":2,
  "version": "1.0",
  "description": "The first extension that I made.",
  "content_security_policy":"default-src 'self' 'unsafe-eval';connect-src *;script-src 'self' 'unsafe-eval';object-src 'self'",
  "browser_action": {
    "default_icon": "img/icon.png",
    "default_popup": "popup.html"
  },
  "options_page":"option.html",
  "permissions": [
    "tabs",
    "http://*/"
  ],
  "background":{
    "scripts": ["./js/background.js"]
  }

}
